{
  "name": "hyper-scheduler",
  "description": "Hyper Scheduler Management",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "test": "node ./node_modules/.bin/jest",
    "lint": "eslint srv/**/*.js --no-error-on-unmatched-pattern --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm run test -- --coverage",
    "format": "npx prettier -w src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VirtualOctopus/VirtualOctopusJS.git"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "^0",
    "@cap-js/sqlite": "^1.0.0",
    "@sap/eslint-plugin-cds": "^2.6.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.0.0",
    "axios": "^1.4.0",
    "better-sqlite3": "^11.0.0",
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "node-test-github-reporter": "^1.1.3",
    "prettier": "^3.0.0"
  },
  "jest": {
    "testTimeout": 30000,
    "collectCoverageFrom": [
      "src/**/*",
      "!**/node_modules/**"
    ],
    "transform": {},
    "testEnvironment": "node",
    "testRegex": "/test/.*\\.test\\.cjs$",
    "moduleFileExtensions": [
      "cjs",
      "js",
      "mjs",
      "json"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@newdash/newdash": "^5.22.0",
    "@sap/cds": "^8.0.0",
    "commonjs-extension-resolution-loader": "^0.1.0",
    "cron": "^4.0.0",
    "cron-parser": "^4.8.1",
    "express": "^4.18.2",
    "generic-pool": "^3.9.0",
    "quickjs-emscripten": "^0.31.0"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  }
}
