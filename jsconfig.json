{
  "compilerOptions": {
    "newLine": "lf",
    "target": "ES2021",
    "module": "CommonJS",
    "outDir": "lib",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": true,
    "lib": [
      "ES6"
    ],
    "checkJs": false,
    "paths": {
      "#cds-models/*": [
        "./@cds-models/*"
      ]
    }
  },
  "include": [
    "srv"
  ],
  "exclude": [
    "commitlint.config.js"
  ]
}